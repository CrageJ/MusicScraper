<script>
function closeAdhesion(ele)
{
    ele.parentElement.style.display = "none";
    var expiryDate = new Date();
    expiryDate.setMinutes(expiryDate.getMinutes() + 15);
    var cookieValue = escape("hide") + "; expires=" + expiryDate.toUTCString();
    document.cookie= "oko-ahdesion=" + cookieValue;
}
</script>

