<script>
var $k = jQuery.noConflict();
$k(document).ready(function() {
    $k('li.dropdown :first-child').click(function() {
        var $el = $k(this).parent();
        if ($el.hasClass('open')) {
            var $a = $el.children('a.dropdown-toggle');
            if ($a.length && $a.attr('href')) {
                location.href = $a.attr('href');
            }
        }
    });
});
</script>

